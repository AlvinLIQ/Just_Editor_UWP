<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Just_Editor_UWP">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="BackBrush" Color="#60787878"/>
            <SolidColorBrush x:Key="CoverBrush" Color="#55121212"/>
            <SolidColorBrush x:Key="SelectedBrush" Color="#CC343434"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="BackBrush" Color="#60FFFFFF"/>
            <SolidColorBrush x:Key="CoverBrush" Color="#55707070"/>
            <SolidColorBrush x:Key="SelectedBrush" Color="#CCCBCBCB"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
